
loat sinVal;
int ledPin = 10;
int toneVal;

void setup(){
     pinMode(8, OUTPUT);
     pinMode(ledPin, OUTPUT);
}

void loop(){
     for(int x=0; x<180; x++){
            sinVal = (sin(x*(3.1412/180)));
            toneVal = 1000+(int(sinVal*1000));
            tone(8, toneVal);
            delay(2); 
     }
 digitalWrite(ledPin,HIGH); 
   delay(2); 
 
  
digitalWrite(ledPin,LOW);
   delay(2); 
     }   
f
